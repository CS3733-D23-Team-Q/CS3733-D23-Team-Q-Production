<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="900.0" prefWidth="512.0" style="-fx-background-color: #F1F1F1;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D23.teamQ.controllers.StatisticsController">
   <children>
      <VBox layoutX="17.0" layoutY="17.0" spacing="48.0">
         <children>
            <StackPane prefHeight="50.0" style="-fx-background-color: #012d5a; -fx-background-radius: 16; -fx-border-radius: 16;">
               <children>
                  <Text fill="#f1f1f1" strokeType="OUTSIDE" strokeWidth="0.0" text="Statistics">
                     <font>
                        <Font name="Roboto Bold" size="28.0" />
                     </font>
                  </Text>
               </children>
            </StackPane>
            <VBox spacing="48.0">
               <children>
                  <Label fx:id="users" alignment="CENTER" prefHeight="50.0" prefWidth="475.0" text="Total users: " textFill="#012d5a">
                     <font>
                        <Font name="Roboto" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="nodes" alignment="CENTER" prefHeight="50.0" prefWidth="475.0" text="Total nodes: " textFill="#012d5a">
                     <font>
                        <Font name="Roboto" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="requests" alignment="CENTER" prefHeight="50.0" prefWidth="475.0" text="Total service requests: " textFill="#012d5a">
                     <font>
                        <Font name="Roboto" size="16.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
