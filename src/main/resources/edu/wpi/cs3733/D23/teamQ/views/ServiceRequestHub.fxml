<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox alignment="TOP_CENTER" spacing="48.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.cs3733.D23.teamQ.controllers.ServiceRequestHubController">
   <children>
      <StackPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="475.0" style="-fx-background-color: #012d5a; -fx-background-radius: 16; -fx-border-radius: 16;">
         <children>
            <Text fill="#f1f1f1" strokeType="OUTSIDE" strokeWidth="0.0" text="Service Request">
               <font>
                  <Font name="Roboto Bold" size="28.0" />
               </font>
            </Text>
         </children>
      </StackPane>
      <VBox alignment="TOP_CENTER" spacing="24.0">
         <children>
            <MFXButton fx:id="ccrButton" onAction="#ccrButtonClicked" prefHeight="50.0" prefWidth="475.0" rippleColor="#e5e5e5" style="-fx-border-color: #000000; -fx-border-radius: 16; -fx-background-radius: 16;" styleClass="mfx-button" text="New Conference Room Request">
               <font>
                  <Font name="Roboto Bold" size="16.0" />
               </font>
            </MFXButton>
            <MFXButton fx:id="frdButton" onAction="#frdButtonClicked" prefHeight="50.0" prefWidth="475.0" rippleColor="#e5e5e5" style="-fx-border-color: #000000; -fx-border-radius: 16; -fx-background-radius: 16;" styleClass="mfx-button" text="New Flower Delivery Request">
               <font>
                  <Font name="Roboto Bold" size="16.0" />
               </font>
            </MFXButton>
            <MFXButton fx:id="ospButton" onAction="#ospButtonClicked" prefHeight="50.0" prefWidth="475.0" rippleColor="#e5e5e5" style="-fx-border-color: #000000; -fx-border-radius: 16; -fx-background-radius: 16;" styleClass="mfx-button" text="New Office Supply Request">
               <font>
                  <Font name="Roboto Bold" size="16.0" />
               </font>
            </MFXButton>
            <MFXButton fx:id="fedButton" onAction="#fedButtonClicked" prefHeight="50.0" prefWidth="475.0" rippleColor="#e5e5e5" style="-fx-border-color: #000000; -fx-border-radius: 16; -fx-background-radius: 16;" styleClass="mfx-button" text="New Furniture Request">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </MFXButton>
            <MFXButton fx:id="mdButton" onAction="#mdButtonClicked" prefHeight="50.0" prefWidth="475.0" rippleColor="#e5e5e5" style="-fx-border-color: #000000; -fx-border-radius: 16; -fx-background-radius: 16;" styleClass="mfx-button" text="New Meal Delivery Request">
               <font>
                  <Font name="Roboto Bold" size="16.0" />
               </font>
            </MFXButton>
         </children>
      </VBox>
   </children>
</VBox>
